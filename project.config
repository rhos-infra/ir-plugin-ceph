[project]
	description = Infrared plugin that configures and involkes ceph-ansible.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group @rhos-ci
[access "refs/for/refs/*"]
	read = group @rhos-ci
	push = group @rhos-ci
	label-Code-Review = -2..+2 group @rhos-ci
	label-Verified = -1..+1 group @rhos-ci
	rebase = group @rhos-ci
	removeReviewer = group @rhos-ci
	submit = group @rhos-ci
[access "refs/heads/*"]
	read = group @rhos-ci
	create = group @rhos-ci
	pushMerge = group @rhos-ci
[access "refs/tags/*"]
	push = group @rhos-ci
