[project]
	description = Infrared plugin that configures and involkes ceph-ansible.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group @rhos-ci
	owner = group user/David Sariel (dsariel)
[access "refs/for/refs/*"]
	read = group @rhos-ci
	read = group user/David Sariel (dsariel)
	push = group @rhos-ci
	label-Code-Review = -2..+2 group @rhos-ci
	label-Code-Review = -2..+2 group user/David Sariel (dsariel)
	label-Verified = -1..+1 group @rhos-ci
	label-Verified = -1..+1 group user/David Sariel (dsariel)
	rebase = group @rhos-ci
	rebase = group user/David Sariel (dsariel)
	removeReviewer = group @rhos-ci
	removeReviewer = group user/David Sariel (dsariel)
	submit = group @rhos-ci
	submit = group user/David Sariel (dsariel)
[access "refs/heads/*"]
	read = group @rhos-ci
	read = group user/David Sariel (dsariel)
	create = group @rhos-ci
	create = group user/David Sariel (dsariel)
	pushMerge = group @rhos-ci
	pushMerge = group user/David Sariel (dsariel)
[access "refs/tags/*"]
	push = group @rhos-ci
	push = group user/David Sariel (dsariel)
	push = +force group user/David Sariel (dsariel)
